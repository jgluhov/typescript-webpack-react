{
  "compilerOptions": {
    "alwaysStrict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "noImplicitAny": true,
    "jsx": "react",
    "module": "commonjs",
    "outDir": "./public/",
    "preserveConstEnums": true,
    "removeComments": true,
    "target": "ES5",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "types": [
      "react",
      "react-dom"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}